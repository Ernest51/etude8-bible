{
  "version": 2,
  "name": "etude8-bible",
  "buildCommand": "cd frontend && DISABLE_ESLINT_PLUGIN=true CI=false yarn build",
  "outputDirectory": "frontend/build",
  "installCommand": "cd frontend && yarn install",
  "env": {
    "REACT_APP_BACKEND_URL": "https://bible-gemini-app.preview.emergentagent.com",
    "DISABLE_ESLINT_PLUGIN": "true",
    "CI": "false"
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}